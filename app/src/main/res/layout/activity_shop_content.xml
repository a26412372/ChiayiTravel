<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ShopContentActivity">
    <TextView
            android:text="店家名稱"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView15"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" android:layout_marginLeft="165dp" android:layout_marginStart="165dp"/>

    <TextView
            android:text="描述"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView17"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="103dp"
            app:layout_constraintEnd_toStartOf="@+id/tv_shop_description"/>
    <TextView
            android:text="地址"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView18"
            android:layout_marginTop="35dp"
            app:layout_constraintTop_toBottomOf="@+id/textView19"
            app:layout_constraintEnd_toStartOf="@+id/tv_shop_address"/>
    <TextView
            android:text="電話"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView19"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_shop_description"
            app:layout_constraintEnd_toStartOf="@+id/tv_shop_phone"/>
    <TextView
            android:text="營業時間"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView20"
            android:layout_marginTop="11dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_shop_website"
            app:layout_constraintStart_toEndOf="@+id/textView21" app:layout_constraintEnd_toEndOf="@+id/textView21"/>
    <TextView
            android:text="官網"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView21"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="34dp"
            android:layout_marginLeft="45dp" android:layout_marginStart="45dp"
            app:layout_constraintTop_toBottomOf="@+id/textView18"/>
    <TextView
            android:text="相關文章"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView22"
            app:layout_constraintStart_toStartOf="@+id/tv_shop_article" android:layout_marginTop="13dp"
            app:layout_constraintTop_toBottomOf="@+id/tv_shop_opentime"
            app:layout_constraintEnd_toStartOf="@+id/tv_shop_article"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_shop_name"
            android:gravity="center"
            app:layout_constraintStart_toStartOf="@+id/textView15" android:layout_marginTop="26dp"
            app:layout_constraintTop_toBottomOf="@+id/textView15"/>

    <TextView
            android:text="TextView"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/tv_shop_description"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/textView17"
            app:layout_constraintStart_toStartOf="@+id/tv_shop_website" android:layout_marginLeft="1dp"
            android:layout_marginStart="1dp" app:layout_constraintEnd_toEndOf="@+id/tv_shop_name"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_shop_phone"
            app:layout_constraintStart_toEndOf="@+id/textView18" android:layout_marginBottom="7dp"
            app:layout_constraintBottom_toTopOf="@+id/textView18"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_shop_address"
            app:layout_constraintStart_toEndOf="@+id/textView21" android:layout_marginBottom="7dp"
            app:layout_constraintBottom_toTopOf="@+id/textView21"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_shop_website"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView21" app:layout_constraintStart_toEndOf="@+id/textView20"
            app:layout_constraintEnd_toEndOf="@+id/textView20"/>
    <TextView
            android:text="TextView"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:id="@+id/tv_shop_opentime"
            android:layout_marginTop="11dp"
            app:layout_constraintTop_toBottomOf="@+id/textView20"
            app:layout_constraintEnd_toEndOf="@+id/tv_shop_description"
            app:layout_constraintStart_toStartOf="@+id/tv_shop_article" android:layout_marginLeft="1dp"
            android:layout_marginStart="1dp" android:layout_marginEnd="1dp" android:layout_marginRight="1dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_shop_article"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="14dp"
            android:layout_marginLeft="71dp" android:layout_marginStart="71dp"
            app:layout_constraintTop_toBottomOf="@+id/textView22"/>
</android.support.constraint.ConstraintLayout>